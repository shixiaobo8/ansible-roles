[mysqld]
#daemonize = on
user = {{ user }}
port = {{ port }}
basedir = {{ mysql_base_dir }}
datadir = {{ mysql_data_dir }}
socket = {{ mysql_socket }}
bind-address = 0.0.0.0
pid-file = {{ mysql_pid_file }}
init_connect='SET NAMES utf8'
character-set-server = utf8
collation-server = utf8_general_ci
max_connections = 2408
log-error = {{ log_path }}
